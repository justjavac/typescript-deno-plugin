  "author": "justjavac <justjavac@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/justjavac/typescript-deno-plugin.git"
  },
  "dependencies": {
    "import-maps": "^0.2.1",
    "merge-deep": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.1",
    "typescript": "^3.9.2"
  }
}
