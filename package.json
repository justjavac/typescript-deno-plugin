{
  "name": "typescript-deno-plugin",
  "version": "1.31.0",
  "description": "Deno language service plugin for TypeScript",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "scripts": {
    "prepublishOnly": "tsc",
    "compile": "tsc",
